name "Bandaged Triangle"
author "Henry Pickle"
circles {
    A x=-0.5 y=0.0 r=0.7
    B x=0.5 y=0.0 r=0.7
    C x=0.0 y=0.8660254 r=0.7
    SYM x=0.0 y=0.2886751 r=10.0
}
base A B C
twists {
    A A 6
    B B 6
    C C 6
    SYM SYM 3 !
}
cut SYM* A3 C
cut SYM* A3 B
cut SYM* A2 C A
cut SYM* A2' B' A
color RED A !B !C
color GREEN B !A !C
color BLUE C !A !B
color BLACK A B C
color MAGENTA A C !B
color CYAN B C !A
color YELLOW A B !C