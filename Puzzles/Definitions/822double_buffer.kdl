name "822 Double Buffer"
author "Hollister Ream"
circles {
    Ac x=-1.3 y=0.0 r=0.8
    Bc x=0.0 y=0.0 r=1.0
    Cc x=1.3 y=0.0 r=0.8
    SYMc x=0.0 y=0.0 r=10.0
}
base Ac Bc Cc
twists {
    A Ac 2
    B Bc 8
    C Cc 2
    SYM SYMc 2 !
}
compounds {
    AB A B
}
colors {
    salmon 250 128 114
    bluey 51 51 255
    pinkish 255 102 255
}
//cut SYM* A* B* A
//cut AB100
cut SYM* A* B* A
color RED Ac !Bc
color YELLOW Bc !Ac
color ORANGE Bc Ac
color BLUE Cc !Bc
color GREEN Bc Cc