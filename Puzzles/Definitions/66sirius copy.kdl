name "Threerius"
author "Henry Pickle"
circles {
    A x=-0.41 y=0.0 r=1.0
    B x=0.41 y=0.0 r=1.0
    C x=0 y="0.82 * sin(pi / 3.)" r=1.0
}
base A B C
twists {
    A A 6
    B B 6
    C C 6
}
compounds {
    AB A B
    BA B A
    BC B C
    CB C B
}
cut AB18 A2 BA15 B2 AB AB15 A2 BA3
cut BC18 B2 CB15 C2 BC BC15 B2 CB3
color RED A
color BLUE B !A
color BLUE C !A